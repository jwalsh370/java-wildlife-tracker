<h4><a href="/">Home</a></h4>

<h1>$ranger.getName()</h1>
<h3>Badge ID: $ranger.getBadgeNumber()</h3>

#if($ranger.getSightings().size() == 0)
  <p>To report a sighting, please visit the <a href="/">home page</a>.</p>
#else
  #foreach($sighting in $ranger.getSightings())
    Animal: $sighting.getAnimal()
    Location: $sighting.getLocation()
    Reported by: $sighting.getRanger()
    ID: $sighting.getId()
  #end
#end
